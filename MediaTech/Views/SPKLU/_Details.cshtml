@model MediaTech.ViewModel.SPKLUViewModel

<div class="modal-header">
    <h5 class="modal-title">Details</h5>
</div>
<div class="modal-body">
    <div class="form-group row">
        <div class="col-md-6">
            @Html.DisplayNameFor(model => model.SPKLUName)
        </div>
        <div class="col-md-6">
            @Html.DisplayFor(model => model.SPKLUName)
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-6">
            @Html.DisplayNameFor(model => model.Status)
        </div>
        <div class="col-md-6">
            @(Model.Status ? "Active" : "Non-Active")
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-6">
            @Html.DisplayNameFor(model => model.SocketType)
        </div>
        <div class="col-md-6">
            @Model.SocketType
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-6">
            @Html.DisplayNameFor(model => model.Alamat)
        </div>
        <div class="col-md-6">
            @Html.DisplayFor(model => model.Alamat)
        </div>
    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-bg-dark text-white" id="btn-cancel">Cancel</button>
</div>